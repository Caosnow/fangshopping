.swiper_container {
  padding: 0 10rpx;
  height: 340rpx;
}
.swiper_container .swiper {
  width: 100%;
  height: 100%;
}
.swiper_container .swiper .image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.menu_container {
  padding: 16rpx 0;
  display: flex;
}
.menu_container .nav_item {
  flex: 1;
  text-align: center;
}
.menu_container .nav_item .image {
  width: 160rpx;
  height: 160rpx;
}
.list_container {
  padding: 10rpx;
  box-sizing: border-box;
}
.list_container .title {
  line-height: 60rpx;
  font-size: 36rpx;
  text-indent: 10rpx;
}
.list_container .list_data {
  display: flex;
  margin-bottom: 10rpx;
}
.list_container .list_data .itemed_left {
  width: 35%;
  height: 100%;
}
.list_container .list_data .itemed_left image {
  width: 100%;
  height: 400rpx;
}
.list_container .list_data .view_box {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-left: 8rpx;
}
.list_container .list_data .itemed_right {
  width: 46%;
}
.list_container .list_data .itemed_right image {
  width: 100%;
  height: 200rpx;
}
.list_container .list_data .itemed_right image:nth-child(1) {
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.list_container .list_data .itemed_right image:nth-child(2) {
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
